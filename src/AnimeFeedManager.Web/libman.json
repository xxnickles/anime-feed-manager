{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "htmx.org@2.0.8",
      "destination": "wwwroot/lib/htmx/",
      "files": [
        "dist/htmx.min.js",
        "dist/htmx.js"
      ]
    },
    {
      "library": "alpinejs@3.15.3",
      "destination": "wwwroot/lib/alpinejs/",
      "files": [
        "dist/cdn.js",
        "dist/cdn.min.js",
        "dist/module.cjs.js",
        "dist/module.cjs.min.js",
        "dist/module.esm.js",
        "dist/module.esm.min.js"
      ]
    },
    {
      "library": "@microsoft/signalr@10.0.0",
      "destination": "wwwroot/lib/signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "@passwordlessdev/passwordless-client@1.2.4",
      "destination": "wwwroot/lib/passwordless-client",
      "files": [
        "dist/umd/passwordless.umd.js",
        "dist/umd/passwordless.umd.min.js",
        "dist/esm/passwordless.mjs",
        "dist/esm/passwordless.min.mjs"
      ]
    }
  ]
}