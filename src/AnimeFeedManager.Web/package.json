{
  "name": "anime-feed-manager-client",
  "version": "1.0.0",
  "description": "A Blazor client + Tailwind",
  "main": "index.js",
  "scripts": {
    "build:js": "node build-js.mjs",
    "watch:js": "node build-js.mjs --watch",
    "build:css": "npx @tailwindcss/cli -i ./Styles/app.css -o ./wwwroot/app.css --minify",
    "watch:css": "npx @tailwindcss/cli -i ./Styles/app.css -o ./wwwroot/app.css --watch",
    "build": "npm run build:css && npm run build:js",
    "watch": "concurrently \"npm run watch:css\" \"npm run watch:js\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "^1.13.5",
    "@tailwindcss/cli": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "daisyui": "^5.1.27",
    "glob": "^11.0.3",
    "tailwindcss": "^4.1.14"
  }
}
